        print(currentOption)
        loadstring(game:HttpGet('https://pastebin.com/raw/PdcNKxUM'))()
    
    
  
  
  end)
    
    --Local Player
    local Tab = Window:NewTab("Player")
    local Section = Tab:NewSection("Walkspeed")
    Section:NewSlider("Walkspeed", "Changes your walkspeed", 250, 16, function(s) 
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)
    local Section = Tab:NewSection("JumpPower")
    Section:NewSlider("JumpPower", "Changes your JumpPower", 300, 32, function(s) 
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
    end)
    
